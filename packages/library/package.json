{
  "name": "@technobuddha/library",
  "version": "1.0.0",
  "description": "A small library of miscellaneous functions",
  "main": "index.js",
  "types": ".",
  "scripts": {
    "clean": "rm -rf *.js *.d.ts docs",
    "build": "tsc",
    "xxprepublishOnly": "npm version patch -m 'Publish Release %s'",
    "version": "npm-run-all clean build document",
    "postversion": "git push && git push --tags",
    "lint": "eslint . --ext '.ts,.tsx,.js'",
    "document": "typedoc --out doc *.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technobuddha/library.git"
  },
  "keywords": [
    "miscellaneous"
  ],
  "author": "Phil Hill <phil@technobuddha.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/technobuddha/library/issues"
  },
  "homepage": "https://github.com/technobuddha/library#readme",
  "dependencies": {
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "chai-almost": "^1.0.1",
    "chai-match": "^1.1.1",
    "eslint": "^7.13.0",
    "eslint-plugin-tsdoc": "^0.2.7",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "ts-mocha": "^8.0.0",
    "typedoc": "^0.20.0-beta.8",
    "typescript": "^4.0.5"
  }
}
